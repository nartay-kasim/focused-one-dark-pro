{
  "name": "Focused Dark Theme",
  "type": "dark",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "class": {
      "foreground": "#e8c53b",
      "fontStyle": "bold underline"
    },
    "enumMember": "#e8c53b",
    "interface": {
      "foreground": "#c296fc"
    },
    "type": {
      "foreground": "#c296fc"
    },
    "enum": {
      "foreground": "#c296fc"
    },
    "namespace": {
      "foreground": "#c296fc"
    },
    "parameter": "#f1ebd4",
    "property": "#f1ebd4"
  },
  "tokenColors": [
    // GENERAL:
    {
      "name": "Token: General Source",
      "scope": "source",
      "settings": {
        "foreground": "#a0a0a0"
      }
    },

    // TYPESCRIPT FILE COLORS:
    {
      "name": "Token: TYPESCRIPT - File",
      "scope": ["source.ts", "meta.block.ts"],
      "settings": {
        "foreground": "#e8c53b88"
      }
    },
    {
      "name": "Token: TYPESCRIPT - Class Name Underline & Bold",
      "scope": ["entity.name.type.class.ts"],
      "settings": {
        "fontStyle": "bold underline"
      }
    },
    {
      "name": "Token: TYPESCRIPT - methods",
      "scope": ["meta.method.declaration.ts", "meta.definition.method.ts"],
      "settings": {
        "foreground": "#f1ebd4"
      }
    },
    {
      "name": "Token: TYPESCRIPT - annotations",
      "scope": ["meta.type.annotation.ts", "entity.name.type.ts"],
      "settings": {
        "foreground": "#f1ebd4"
      }
    },

    // TS FILE FUNCTION COLORS:
    {
      "name": "Token: Functions and Arrows",
      "scope": [
        "meta.definition.function.ts",
        "variable.other.constant.js",
        "meta.definition.function.js"
      ],
      "settings": {
        "foreground": "#08ce01",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Token: Functions and Arrows",
      "scope": ["entity.name.function.ts, meta.definition.variable.ts"],
      "settings": {
        "foreground": "#08ce01",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Token: Functions and Arrows",
      "scope": ["variable.other.constant.ts"],
      "settings": {
        "foreground": "#f1ebd4",
        "fontStyle": "bold"
      }
    },

    // TSX FILE COLORS:
    {
      "name": "Token: TSX - File",
      "scope": ["source.tsx"],
      "settings": {
        "foreground": "#aaa2a7"
      }
    },
    {
      "name": "Token: TSX - HTML Tags",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#f170bf"
      }
    },
    {
      "name": "Token: TSX - Function Components",
      "scope": [
        "entity.name.function.tsx, meta.block.tsx meta.arrow.tsx meta.var.expr.tsx"
      ],
      "settings": {
        "foreground": "#aaa2a7",
        "fontStyle": "bold underline"
      }
    },
    {
      "name": "Token: TSX - Function Names Returning TSX",
      "scope": [
        "entity.name.function.tsx",
        "meta.arrow.tsx entity.name.function.tsx",
        "meta.variable.assignment.tsx entity.name.function",
        "meta.var.expr.tsx entity.name.function"
      ],
      "settings": {
        "foreground": "#f170bf",
        "fontStyle": "bold underline"
      }
    },

    // OTHER, MISCELLANEOUS, UNSUPPORTED:
    {
      "name": "Token: Other",
      "scope": ["*"],
      "settings": {
        "foreground": "#ff5e5e"
      }
    }
  ],
